SQL: INSERT INTO `think_article` (`title`,`bid`,`content`,`sendTime`) VALUES ('近五年中心人员教学科研主要成果','6','<p style=\";padding: 0px;color: rgb(51, 51, 51);font-family: Arial, 宋体, Verdana, Helvetica, sans-serif;font-size: 14px;white-space: normal;line-height: 17.5px\"><span style=\"font-size:18px\"><span style=\"color: black\">1.2012</span><span style=\"font-family:宋体\"><span style=\"font-family: 仿宋_GB2312;color: black\">年省教学成果奖二等奖</span><span style=\"color: black\">1</span><span style=\"font-family: 仿宋_GB2312;color: black\">项；</span><span style=\"color: black\">2012</span><span style=\"font-family: 仿宋_GB2312;color: black\">年广东工业大学优秀教学成果一、二等奖各</span><span style=\"color: black\">1</span><span style=\"font-family: 仿宋_GB2312;color: black\">项；</span></span></span></p><p style=\";padding: 0px;color: rgb(51, 51, 51);font-family: Arial, 宋体, Verdana, Helvetica, sans-serif;font-size: 14px;white-space: normal;line-height: 17.5px\"><span style=\"font-size:18px\"><span style=\"color: black\">2.&nbsp;</span><span style=\"font-family:宋体\"><span style=\"font-family: 仿宋_GB2312;color: black\">承担校级教研课题</span><span style=\"color: black\">5</span><span style=\"font-family: 仿宋_GB2312;color: black\">项；</span></span></span></p><p style=\";padding: 0px;color: rgb(51, 51, 51);font-family: Arial, 宋体, Verdana, Helvetica, sans-serif;font-size: 14px;white-space: normal;line-height: 17.5px\"><span style=\"font-size:18px\"><span style=\"color: black\">3.&nbsp;</span><span style=\"font-family:宋体\"><span style=\"font-family: 仿宋_GB2312;color: black\">发表教研论文</span><span style=\"color: black\">9</span><span style=\"font-family: 仿宋_GB2312;color: black\">篇；</span></span></span></p><p style=\";padding: 0px;color: rgb(51, 51, 51);font-family: Arial, 宋体, Verdana, Helvetica, sans-serif;font-size: 14px;white-space: normal;line-height: 17.5px\"><span style=\"font-size:18px\"><span style=\"color: black\">4.&nbsp;</span><span style=\"font-family:宋体\"><span style=\"font-family: 仿宋_GB2312;color: black\">编写实验讲义</span><span style=\"color: black\">4</span><span style=\"font-family: 仿宋_GB2312;color: black\">本；</span></span></span></p><p style=\";padding: 0px;color: rgb(51, 51, 51);font-family: Arial, 宋体, Verdana, Helvetica, sans-serif;font-size: 14px;white-space: normal;line-height: 17.5px\"><span style=\"font-size:18px\"><span style=\"color: black\">5.&nbsp;</span><span style=\"font-family:宋体\"><span style=\"font-family: 仿宋_GB2312;color: black\">承担科研项目</span><span style=\"color: black\">&nbsp;26</span><span style=\"font-family: 仿宋_GB2312;color: black\">项，其中国家基金</span><span style=\"color: black\">&nbsp;7</span><span style=\"font-family: 仿宋_GB2312;color: black\">项，省基金</span><span style=\"color: black\">9&nbsp;</span><span style=\"font-family: 仿宋_GB2312;color: black\">项，市（厅）级</span><span style=\"color: black\">10&nbsp;</span><span style=\"font-family: 仿宋_GB2312;color: black\">项；总经费</span><span style=\"color: black\">552.5</span><span style=\"font-family: 仿宋_GB2312;color: black\">万元；</span></span></span></p><p style=\";padding: 0px;color: rgb(51, 51, 51);font-family: Arial, 宋体, Verdana, Helvetica, sans-serif;font-size: 14px;white-space: normal;line-height: 17.5px\"><span style=\"font-size:18px\"><span style=\"color: black\">6.&nbsp;</span><span style=\"font-family:宋体\"><span style=\"font-family: 仿宋_GB2312\">发表SCI、EI收录学术论文<span style=\"color: black\">90</span>多篇</span><span style=\"font-family: 仿宋_GB2312;color: black\">；</span></span></span></p><p style=\";padding: 0px;color: rgb(51, 51, 51);font-family: Arial, 宋体, Verdana, Helvetica, sans-serif;font-size: 14px;white-space: normal;line-height: 17.5px\"><span style=\"font-size:18px\"><span style=\"color: black\">7.&nbsp;</span><span style=\"font-family:宋体\"><span style=\"font-family: 仿宋_GB2312;color: black\">指导学生参加“挑战杯”、广东省物理实验设计大赛、全国光电设计大赛，</span><span style=\"line-height: 22.5px;font-family: 仿宋_GB2312\">获国家级奖励13项，获奖学生48人次；获省级奖励52项，获奖学生188人次。</span></span></span></p><p style=\";padding: 0px;color: rgb(51, 51, 51);font-family: Arial, 宋体, V